{
  "presets": [
    "es2015",
    "react",
    "stage-3",
    "stage-0"
  ],
  //import css for antd
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "components": "./src/components"
        }
      }
    ],
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": true
      }
    ],
    // `style: true` for less
    "syntax-dynamic-import"
  ]
}